{
    "name": "grpc-testing/php-api-gateway",
    "description": "PHP API Gateway for gRPC microservices",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "grpc/grpc": "^1.50",
        "google/protobuf": "^3.21",
        "psr/http-message": "^1.0",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "GPBMetadata\\": "src/Generated/GPBMetadata/",
            "Category\\": "src/Generated/Category/",
            "Product\\": "src/Generated/Product/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "serve": "rr serve",
        "dev": "rr serve -c .rr.dev.yaml"
    }
}